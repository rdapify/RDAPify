{
  "version": "1.0.0",
  "description": "Test vectors for domain name queries",
  "updated": "2024-01-24",
  "valid": [
    {
      "id": "domain-001",
      "description": "Simple ASCII domain",
      "input": "example.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "example.com",
        "handle": "EXAMPLE-COM"
      }
    },
    {
      "id": "domain-002",
      "description": "Subdomain",
      "input": "www.example.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "www.example.com"
      }
    },
    {
      "id": "domain-003",
      "description": "Multi-level subdomain",
      "input": "api.v2.example.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "api.v2.example.com"
      }
    },
    {
      "id": "domain-004",
      "description": "Country code TLD",
      "input": "example.co.uk",
      "expected": {
        "objectClass": "domain",
        "ldhName": "example.co.uk"
      }
    },
    {
      "id": "domain-005",
      "description": "New gTLD",
      "input": "example.tech",
      "expected": {
        "objectClass": "domain",
        "ldhName": "example.tech"
      }
    },
    {
      "id": "domain-006",
      "description": "Hyphenated domain",
      "input": "my-example-domain.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "my-example-domain.com"
      }
    },
    {
      "id": "domain-007",
      "description": "Numeric domain",
      "input": "123456.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "123456.com"
      }
    },
    {
      "id": "domain-008",
      "description": "Single character domain",
      "input": "x.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "x.com"
      }
    },
    {
      "id": "domain-009",
      "description": "Maximum length label (63 chars)",
      "input": "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijk.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijk.com"
      }
    },
    {
      "id": "domain-010",
      "description": "IDN domain (Punycode)",
      "input": "xn--e1afmkfd.xn--p1ai",
      "expected": {
        "objectClass": "domain",
        "ldhName": "xn--e1afmkfd.xn--p1ai",
        "unicodeName": "пример.рф"
      }
    },
    {
      "id": "domain-011",
      "description": "Domain with numbers and hyphens",
      "input": "test-123-domain.org",
      "expected": {
        "objectClass": "domain",
        "ldhName": "test-123-domain.org"
      }
    },
    {
      "id": "domain-012",
      "description": "Four-level domain",
      "input": "sub.domain.example.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "sub.domain.example.com"
      }
    },
    {
      "id": "domain-013",
      "description": "Domain with all lowercase",
      "input": "lowercase.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "lowercase.com"
      }
    },
    {
      "id": "domain-014",
      "description": "Domain with mixed case (should normalize)",
      "input": "MixedCase.COM",
      "expected": {
        "objectClass": "domain",
        "ldhName": "mixedcase.com"
      }
    },
    {
      "id": "domain-015",
      "description": "Domain with trailing dot (FQDN)",
      "input": "example.com.",
      "expected": {
        "objectClass": "domain",
        "ldhName": "example.com"
      }
    }
  ],
  "invalid": [
