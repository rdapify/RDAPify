      "error": "ValidationError",
      "message": "Domain exceeds maximum length"
    },
    {
      "id": "domain-err-009",
      "description": "Label exceeding 63 characters",
      "input": "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijkl.com",
      "error": "ValidationError",
      "message": "Label exceeds maximum length"
    },
    {
      "id": "domain-err-010",
      "description": "Domain with underscore",
      "input": "example_domain.com",
      "error": "ValidationError",
      "message": "Domain contains invalid characters"
    },
    {
      "id": "domain-err-011",
      "description": "Missing TLD",
      "input": "example",
      "error": "ValidationError",
      "message": "Domain must have at least one dot"
    },
    {
      "id": "domain-err-012",
      "description": "Numeric TLD",
      "input": "example.123",
      "error": "ValidationError",
      "message": "TLD cannot be numeric"
    },
    {
      "id": "domain-err-013",
      "description": "Domain with whitespace",
      "input": " example.com ",
      "error": "ValidationError",
      "message": "Domain contains whitespace"
    },
    {
      "id": "domain-err-014",
      "description": "Domain with unicode (not punycode)",
      "input": "例え.com",
      "error": "ValidationError",
      "message": "Domain must be ASCII or Punycode"
    },
    {
      "id": "domain-err-015",
      "description": "Null value",
      "input": null,
      "error": "ValidationError",
      "message": "Domain must be a string"
    }
  ],
  "edge_cases": [
    {
      "id": "domain-edge-001",
      "description": "Single letter TLD",
      "input": "example.x",
      "expected": {
        "objectClass": "domain",
        "ldhName": "example.x"
      }
    },
    {
      "id": "domain-edge-002",
      "description": "Two letter domain",
      "input": "ab.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "ab.com"
      }
    },
    {
      "id": "domain-edge-003",
      "description": "All numeric subdomain",
      "input": "123.example.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "123.example.com"
      }
    },
    {
      "id": "domain-edge-004",
      "description": "Domain with maximum labels",
      "input": "a.b.c.d.e.f.g.h.i.j.k.l.m.n.o.p.q.r.s.t.u.v.w.x.y.z.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "a.b.c.d.e.f.g.h.i.j.k.l.m.n.o.p.q.r.s.t.u.v.w.x.y.z.com"
      }
    },
    {
      "id": "domain-edge-005",
      "description": "Domain near maximum length (250 chars)",
      "input": "subdomain.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing.com",
      "expected": {
        "objectClass": "domain",
        "ldhName": "subdomain.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing-purposes-only.example-domain-with-very-long-name-for-testing.com"
      }
    }
  ]
}
