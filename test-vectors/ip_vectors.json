{
  "version": "1.0.0",
  "description": "Test vectors for IP address queries",
  "updated": "2024-01-24",
  "valid": [
    {
      "id": "ip-001",
      "description": "Google DNS IPv4",
      "input": "8.8.8.8",
      "expected": {
        "objectClass": "ip network",
        "startAddress": "8.8.8.0",
        "endAddress": "8.8.8.255",
        "ipVersion": "v4",
        "cidr": "8.8.8.0/24"
      }
    },
    {
      "id": "ip-002",
      "description": "Cloudflare DNS IPv4",
      "input": "1.1.1.1",
      "expected": {
        "objectClass": "ip network",
        "startAddress": "1.1.1.0",
        "endAddress": "1.1.1.255",
        "ipVersion": "v4"
      }
    },
    {
      "id": "ip-003",
      "description": "Private IPv4 (RFC 1918)",
      "input": "192.168.1.1",
      "expected": {
        "objectClass": "ip network",
        "startAddress": "192.168.0.0",
        "endAddress": "192.168.255.255",
        "ipVersion": "v4",
        "cidr": "192.168.0.0/16"
      }
    },
    {
      "id": "ip-004",
      "description": "Localhost IPv4",
      "input": "127.0.0.1",
      "expected": {
        "objectClass": "ip network",
        "startAddress": "127.0.0.0",
        "endAddress": "127.255.255.255",
        "ipVersion": "v4",
        "cidr": "127.0.0.0/8"
      }
    },
    {
      "id": "ip-005",
      "description": "Class A network",
      "input": "10.0.0.1",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v4"
      }
    },
    {
      "id": "ip-006",
      "description": "Class B network",
      "input": "172.16.0.1",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v4"
      }
    },
    {
      "id": "ip-007",
      "description": "Google DNS IPv6",
      "input": "2001:4860:4860::8888",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6",
        "startAddress": "2001:4860:4860::",
        "endAddress": "2001:4860:4860:ffff:ffff:ffff:ffff:ffff"
      }
    },
    {
      "id": "ip-008",
      "description": "Cloudflare DNS IPv6",
      "input": "2606:4700:4700::1111",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6"
      }
    },
    {
      "id": "ip-009",
      "description": "IPv6 localhost",
      "input": "::1",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6",
        "startAddress": "::1",
        "endAddress": "::1"
      }
    },
    {
      "id": "ip-010",
      "description": "IPv6 unspecified",
      "input": "::",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6"
      }
    },
    {
      "id": "ip-011",
      "description": "IPv6 with full notation",
      "input": "2001:0db8:0000:0000:0000:0000:0000:0001",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6",
        "startAddress": "2001:db8::"
      }
    },
    {
      "id": "ip-012",
      "description": "IPv6 compressed notation",
      "input": "2001:db8::1",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v6"
      }
    },
    {
      "id": "ip-013",
      "description": "IPv4 with leading zeros",
      "input": "008.008.008.008",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v4",
        "startAddress": "8.8.8.0"
      }
    },
    {
      "id": "ip-014",
      "description": "IPv4 broadcast address",
      "input": "255.255.255.255",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v4"
      }
    },
    {
      "id": "ip-015",
      "description": "IPv4 network address",
      "input": "0.0.0.0",
      "expected": {
        "objectClass": "ip network",
        "ipVersion": "v4"
      }
    }
  ],
